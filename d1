Count_WK1 =
VAR UniqueWeeks = VALUES('Table'[WeekNumber])
RETURN
SUMX(
    UniqueWeeks,
    CALCULATE(
        COUNTROWS('Table'),
        FILTER(
            ALL('Table'),
            'Table'[WeekNumber] = EARLIER(UniqueWeeks[WeekNumber]) &&
            'Table'[Date] >= [_01WK1_Start] &&
            'Table'[Date] <= [_02WK1_End]
        )
    )
)
