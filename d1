__Count_WK1 = 
CALCULATE(
    COUNTROWS('Table'),
    FILTER(
        ALL('Table'),
        'Table'[Date] >= [_01WK1_Start] && 
        'Table'[Date] <= [_02WK1_End]
    )
)

_01_Count_WK1 = 
CALCULATE(
    COUNTROWS(
        FILTER(
            ALL('Table'),
            'Table'[Date] >= [_01WK1_Start] && 
            'Table'[Date] <= [_02WK1_End]
        )
    )
)
